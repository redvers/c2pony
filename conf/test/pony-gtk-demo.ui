<?xml version='1.0' encoding='UTF-8'?>
<!-- Created with Cambalache 0.10.3 -->
<interface>
  <!-- interface-name pony-gtk-demo.ui -->
  <requires lib="gtk" version="4.6"/>
  <object class="GtkApplicationWindow" id="window">
    <property name="show-menubar">True</property>
    <child>
      <object class="GtkPaned">
        <property name="vexpand">True</property>
        <child>
          <object class="GtkScrolledWindow">
            <child>
              <object class="GtkListView" id="demolistview"/>
            </child>
          </object>
        </child>
        <child>
          <object class="GtkNotebook" id="notebook">
            <property name="enable-popup">True</property>
            <property name="hexpand">True</property>
            <property name="scrollable">True</property>
            <property name="vexpand">True</property>
            <child>
              <object class="GtkNotebookPage">
                <property name="child">
                  <object class="GtkScrolledWindow">
                    <property name="child">
                      <object class="GtkTextView" id="info-textview">
                        <property name="buffer">infobuffer</property>
                        <property name="halign">baseline</property>
                        <property name="hexpand">True</property>
                        <property name="monospace">True</property>
                        <property name="valign">start</property>
                        <property name="vexpand">True</property>
                        <property name="wrap-mode">char</property>
                      </object>
                    </property>
                    <property name="vexpand">True</property>
                  </object>
                </property>
                <property name="tab-expand">True</property>
                <property name="tab-label">Info</property>
              </object>
            </child>
          </object>
        </child>
      </object>
    </child>
  </object>
  <object class="GtkTextBuffer" id="infobuffer">
    <property name="text">demo info</property>
  </object>
</interface>
