
use @gtk_expression_get_type[U64]()

use "../GLib"
use "../GObject"
use "../Gio"
use "lib:gtk-4"
    
class GtkExpression is GtkExpressionI
  var ptr: NullablePointer[GtkExpressionT]

  new create_from_ptr(ptr': NullablePointer[GtkExpressionT]) => ptr = ptr'

  new create() =>
    ptr = GObjectG.gnew(@gtk_expression_get_type())
    GObjectP.set_data_p[String](ptr, "_PonyGObjectTypeString_", "GtkExpression")
    GObjectP.set_data_p[GtkExpression](ptr, "_PonyGObjectPonyObject_", this)

  // GtkExpression
  fun ref getptr(): NullablePointer[GtkExpressionT] => ptr
