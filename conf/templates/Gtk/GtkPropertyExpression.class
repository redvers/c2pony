
use @gtk_property_expression_get_type[U64]()

use "../GLib"
use "../GObject"
use "../Gio"
use "lib:gtk-4"
    
class GtkPropertyExpression is GtkPropertyExpressionI
  var ptr: NullablePointer[GtkExpressionT]

  new create_from_ptr(ptr': NullablePointer[GtkExpressionT]) => ptr = ptr'

  new create() =>
    ptr = GObjectG.gnew(@gtk_property_expression_get_type())
    GObjectP.set_data_p[String](ptr, "_PonyGObjectTypeString_", "GtkPropertyExpression")
    GObjectP.set_data_p[GtkPropertyExpression](ptr, "_PonyGObjectPonyObject_", this)

  // GtkPropertyExpression
  fun ref getptr(): NullablePointer[GtkExpressionT] => ptr
