
use @gtk_object_expression_get_type[U64]()

use "../GLib"
use "../GObject"
use "../Gio"
use "lib:gtk-4"
    
class GtkObjectExpression is GtkObjectExpressionI
  var ptr: NullablePointer[GtkExpressionT]

  new create_from_ptr(ptr': NullablePointer[GtkExpressionT]) => ptr = ptr'

  new create() =>
    ptr = GObjectG.gnew(@gtk_object_expression_get_type())
    GObjectP.set_data_p[String](ptr, "_PonyGObjectTypeString_", "GtkObjectExpression")
    GObjectP.set_data_p[GtkObjectExpression](ptr, "_PonyGObjectPonyObject_", this)

  // GtkObjectExpression
  fun ref getptr(): NullablePointer[GtkExpressionT] => ptr
