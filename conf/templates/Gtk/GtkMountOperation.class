
use @gtk_mount_operation_get_type[U64]()

use "../GLib"
use "../GObject"
use "../Gio"
use "lib:gtk-4"
    
class GtkMountOperation is GtkMountOperationI
  var ptr: NullablePointer[GObjectT]

  new create_from_ptr(ptr': NullablePointer[GObjectT]) => ptr = ptr'

  new create() =>
    ptr = GObjectG.gnew(@gtk_mount_operation_get_type())
    GObjectP.set_data_p[String](ptr, "_PonyGObjectTypeString_", "GtkMountOperation")
    GObjectP.set_data_p[GtkMountOperation](ptr, "_PonyGObjectPonyObject_", this)

  // GtkMountOperation
  fun ref getptr(): NullablePointer[GObjectT] => ptr
