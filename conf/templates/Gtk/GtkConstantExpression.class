
use @gtk_constant_expression_get_type[U64]()

use "../GLib"
use "../GObject"
use "../Gio"
use "lib:gtk-4"
    
class GtkConstantExpression is GtkConstantExpressionI
  var ptr: Pointer[GtkExpressionP]

  new create_from_ptr(ptr': Pointer[GtkExpressionP])? => 
    if (ptr'.is_null()) then error else ptr = ptr' end
// gtk_constant_expression_get_type
  new create() =>
    ptr = GObjectG.gnew(@gtk_constant_expression_get_type())

  fun ref getptr(): Pointer[GtkExpressionP] => ptr
