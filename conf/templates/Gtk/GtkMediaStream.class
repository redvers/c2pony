
use @gtk_media_stream_get_type[U64]()

use "../GLib"
use "../GObject"
use "../Gio"
use "lib:gtk-4"
    
class GtkMediaStream is GtkMediaStreamI
  var ptr: NullablePointer[GObjectT]

  new create_from_ptr(ptr': NullablePointer[GObjectT]) => ptr = ptr'

  new create() =>
    ptr = GObjectG.gnew(@gtk_media_stream_get_type())
    GObjectP.set_data_p[String](ptr, "_PonyGObjectTypeString_", "GtkMediaStream")
    GObjectP.set_data_p[GtkMediaStream](ptr, "_PonyGObjectPonyObject_", this)

  // GtkMediaStream
  fun ref getptr(): NullablePointer[GObjectT] => ptr
