
use @gtk_signal_action_get_type[U64]()

use "../GLib"
use "../GObject"
use "../Gio"
use "lib:gtk-4"
    
class GtkSignalAction is GtkSignalActionI
  var ptr: NullablePointer[GObjectT]

  new create_from_ptr(ptr': NullablePointer[GObjectT]) => ptr = ptr'

  new create() =>
    ptr = GObjectG.gnew(@gtk_signal_action_get_type())
    GObjectP.set_data_p[String](ptr, "_PonyGObjectTypeString_", "GtkSignalAction")
    GObjectP.set_data_p[GtkSignalAction](ptr, "_PonyGObjectPonyObject_", this)

  // GtkSignalAction
  fun ref getptr(): NullablePointer[GObjectT] => ptr
