
use @gtk_closure_expression_get_type[U64]()

use "../GLib"
use "../GObject"
use "../Gio"
use "lib:gtk-4"
    
class GtkClosureExpression is GtkClosureExpressionI
  var ptr: NullablePointer[GtkExpressionT]

  new create_from_ptr(ptr': NullablePointer[GtkExpressionT]) => ptr = ptr'

  new create() =>
    ptr = GObjectG.gnew(@gtk_closure_expression_get_type())

  // GtkClosureExpression
  fun ref getptr(): NullablePointer[GtkExpressionT] => ptr
