
use @g_memory_output_stream_get_type[U64]()

use "../GLib"
use "../GObject"
use "lib:gio-2.0"

class GMemoryOutputStream is GMemoryOutputStreamI
  var ptr: NullablePointer[GObjectT]

  new create_from_ptr(ptr': NullablePointer[GObjectT]) => ptr = ptr'

  new create() =>
    ptr = GObjectG.gnew(@g_memory_output_stream_get_type())

  // GMemoryOutputStream
  fun ref getptr(): NullablePointer[GObjectT] => ptr
