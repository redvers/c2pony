
use @g_dbus_connection_get_type[U64]()

use "../GLib"
use "../GObject"
use "lib:gio-2.0"

class GDBusConnection is GDBusConnectionI
  var ptr: NullablePointer[GObjectT]

  new create_from_ptr(ptr': NullablePointer[GObjectT]) => ptr = ptr'

  new create() =>
    ptr = GObjectG.gnew(@g_dbus_connection_get_type())
    GObjectP.set_data_p[String](ptr, "_PonyGObjectTypeString_", "GDBusConnection")
    GObjectP.set_data_p[GDBusConnection](ptr, "_PonyGObjectPonyObject_", this)

  // GDBusConnection
  fun ref getptr(): NullablePointer[GObjectT] => ptr
