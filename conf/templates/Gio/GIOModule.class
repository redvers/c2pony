
use @g_io_module_get_type[U64]()

use "../GLib"
use "../GObject"
use "lib:gio-2.0"

class GIOModule is GIOModuleI
  var ptr: NullablePointer[GObjectT]

  new create_from_ptr(ptr': NullablePointer[GObjectT]) => ptr = ptr'

  new create() =>
    ptr = GObjectG.gnew(@g_io_module_get_type())
    GObjectP.set_data_p[String](ptr, "_PonyGObjectTypeString_", "GIOModule")
    GObjectP.set_data_p[GIOModule](ptr, "_PonyGObjectPonyObject_", this)

  // GIOModule
  fun ref getptr(): NullablePointer[GObjectT] => ptr
