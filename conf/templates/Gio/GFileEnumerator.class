
use @g_file_enumerator_get_type[U64]()

use "../GLib"
use "../GObject"
use "lib:gio-2.0"

class GFileEnumerator is GFileEnumeratorI
  var ptr: NullablePointer[GObjectT]

  new create_from_ptr(ptr': NullablePointer[GObjectT]) => ptr = ptr'

  new create() =>
    ptr = GObjectG.gnew(@g_file_enumerator_get_type())
    GObjectP.set_data_p[String](ptr, "_PonyGObjectTypeString_", "GFileEnumerator")
    GObjectP.set_data_p[GFileEnumerator](ptr, "_PonyGObjectPonyObject_", this)

  // GFileEnumerator
  fun ref getptr(): NullablePointer[GObjectT] => ptr
