
use @g_output_stream_get_type[U64]()

use "../GLib"
use "../GObject"
use "lib:gio-2.0"

class GOutputStream is GOutputStreamI
  var ptr: NullablePointer[GObjectT]

  new create_from_ptr(ptr': NullablePointer[GObjectT]) => ptr = ptr'

  new create() =>
    ptr = GObjectG.gnew(@g_output_stream_get_type())
    GObjectP.set_data_p[String](ptr, "_PonyGObjectTypeString_", "GOutputStream")
    GObjectP.set_data_p[GOutputStream](ptr, "_PonyGObjectPonyObject_", this)

  // GOutputStream
  fun ref getptr(): NullablePointer[GObjectT] => ptr
