
use @g_app_launch_context_get_type[U64]()

use "../GLib"
use "../GObject"
use "lib:gio-2.0"

class GAppLaunchContext is GAppLaunchContextI
  var ptr: NullablePointer[GObjectT]

  new create_from_ptr(ptr': NullablePointer[GObjectT]) => ptr = ptr'

  new create() =>
    ptr = GObjectG.gnew(@g_app_launch_context_get_type())

  fun ref getptr(): NullablePointer[GObjectT] => ptr
