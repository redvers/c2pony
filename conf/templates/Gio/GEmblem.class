
use @g_emblem_get_type[U64]()

use "../GLib"
use "../GObject"
use "lib:gio-2.0"

class GEmblem is GEmblemI
  var ptr: NullablePointer[GObjectT]

  new create_from_ptr(ptr': NullablePointer[GObjectT]) => ptr = ptr'

  new create() =>
    ptr = GObjectG.gnew(@g_emblem_get_type())
    GObjectP.set_data_p[String](ptr, "_PonyGObjectTypeString_", "GEmblem")
    GObjectP.set_data_p[GEmblem](ptr, "_PonyGObjectPonyObject_", this)

  // GEmblem
  fun ref getptr(): NullablePointer[GObjectT] => ptr
