
use @g_task_get_type[U64]()

use "../GLib"
use "../GObject"
use "lib:gio-2.0"

class GTask is GTaskI
  var ptr: NullablePointer[GObjectT]

  new create_from_ptr(ptr': NullablePointer[GObjectT]) => ptr = ptr'

  new create() =>
    ptr = GObjectG.gnew(@g_task_get_type())
    GObjectP.set_data_p[String](ptr, "_PonyGObjectTypeString_", "GTask")
    GObjectP.set_data_p[GTask](ptr, "_PonyGObjectPonyObject_", this)

  // GTask
  fun ref getptr(): NullablePointer[GObjectT] => ptr
