
use @g_subprocess_get_type[U64]()

use "../GLib"
use "../GObject"
use "lib:gio-2.0"

class GSubprocess is GSubprocessI
  var ptr: NullablePointer[GObjectT]

  new create_from_ptr(ptr': NullablePointer[GObjectT]) => ptr = ptr'

  new create() =>
    ptr = GObjectG.gnew(@g_subprocess_get_type())
    GObjectP.set_data_p[String](ptr, "_PonyGObjectTypeString_", "GSubprocess")
    GObjectP.set_data_p[GSubprocess](ptr, "_PonyGObjectPonyObject_", this)

  // GSubprocess
  fun ref getptr(): NullablePointer[GObjectT] => ptr
