
use @g_filter_output_stream_get_type[U64]()

use "../GLib"
use "../GObject"
use "lib:gio-2.0"

class GFilterOutputStream is GFilterOutputStreamI
  var ptr: NullablePointer[GObjectT]

  new create_from_ptr(ptr': NullablePointer[GObjectT]) => ptr = ptr'

  new create() =>
    ptr = GObjectG.gnew(@g_filter_output_stream_get_type())
    GObjectP.set_data_p[String](ptr, "_PonyGObjectTypeString_", "GFilterOutputStream")
    GObjectP.set_data_p[GFilterOutputStream](ptr, "_PonyGObjectPonyObject_", this)

  // GFilterOutputStream
  fun ref getptr(): NullablePointer[GObjectT] => ptr
