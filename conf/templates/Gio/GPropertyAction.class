
use @g_property_action_get_type[U64]()

use "../GLib"
use "../GObject"
use "lib:gio-2.0"

class GPropertyAction is GPropertyActionI
  var ptr: NullablePointer[GObjectT]

  new create_from_ptr(ptr': NullablePointer[GObjectT]) => ptr = ptr'

  new create() =>
    ptr = GObjectG.gnew(@g_property_action_get_type())
    GObjectP.set_data_p[String](ptr, "_PonyGObjectTypeString_", "GPropertyAction")
    GObjectP.set_data_p[GPropertyAction](ptr, "_PonyGObjectPonyObject_", this)

  // GPropertyAction
  fun ref getptr(): NullablePointer[GObjectT] => ptr
