
use @g_inet_address_get_type[U64]()

use "../GLib"
use "../GObject"
use "lib:gio-2.0"

class GInetAddress is GInetAddressI
  var ptr: NullablePointer[GObjectT]

  new create_from_ptr(ptr': NullablePointer[GObjectT]) => ptr = ptr'

  new create() =>
    ptr = GObjectG.gnew(@g_inet_address_get_type())
    GObjectP.set_data_p[String](ptr, "_PonyGObjectTypeString_", "GInetAddress")
    GObjectP.set_data_p[GInetAddress](ptr, "_PonyGObjectPonyObject_", this)

  // GInetAddress
  fun ref getptr(): NullablePointer[GObjectT] => ptr
