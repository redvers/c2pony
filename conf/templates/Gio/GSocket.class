
use @g_socket_get_type[U64]()

use "../GLib"
use "../GObject"
use "lib:gio-2.0"

class GSocket is GSocketI
  var ptr: NullablePointer[GObjectT]

  new create_from_ptr(ptr': NullablePointer[GObjectT]) => ptr = ptr'

  new create() =>
    ptr = GObjectG.gnew(@g_socket_get_type())
    GObjectP.set_data_p[String](ptr, "_PonyGObjectTypeString_", "GSocket")
    GObjectP.set_data_p[GSocket](ptr, "_PonyGObjectPonyObject_", this)

  // GSocket
  fun ref getptr(): NullablePointer[GObjectT] => ptr
