
use @g_simple_action_get_type[U64]()

use "../GLib"
use "../GObject"
use "lib:gio-2.0"

class GSimpleAction is GSimpleActionI
  var ptr: NullablePointer[GObjectT]

  new create_from_ptr(ptr': NullablePointer[GObjectT]) => ptr = ptr'

  new create() =>
    ptr = GObjectG.gnew(@g_simple_action_get_type())
    GObjectP.set_data_p[String](ptr, "_PonyGObjectTypeString_", "GSimpleAction")
    GObjectP.set_data_p[GSimpleAction](ptr, "_PonyGObjectPonyObject_", this)

  // GSimpleAction
  fun ref getptr(): NullablePointer[GObjectT] => ptr
