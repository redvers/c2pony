
use @g_dbus_object_skeleton_get_type[U64]()

use "../GLib"
use "../GObject"
use "lib:gio-2.0"

class GDBusObjectSkeleton is GDBusObjectSkeletonI
  var ptr: NullablePointer[GObjectT]

  new create_from_ptr(ptr': NullablePointer[GObjectT]) => ptr = ptr'

  new create() =>
    ptr = GObjectG.gnew(@g_dbus_object_skeleton_get_type())
    GObjectP.set_data_p[String](ptr, "_PonyGObjectTypeString_", "GDBusObjectSkeleton")
    GObjectP.set_data_p[GDBusObjectSkeleton](ptr, "_PonyGObjectPonyObject_", this)

  // GDBusObjectSkeleton
  fun ref getptr(): NullablePointer[GObjectT] => ptr
