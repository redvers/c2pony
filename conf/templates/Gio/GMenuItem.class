
use @g_menu_item_get_type[U64]()

use "../GLib"
use "../GObject"
use "lib:gio-2.0"

class GMenuItem is GMenuItemI
  var ptr: NullablePointer[GObjectT]

  new create_from_ptr(ptr': NullablePointer[GObjectT]) => ptr = ptr'

  new create() =>
    ptr = GObjectG.gnew(@g_menu_item_get_type())
    GObjectP.set_data_p[String](ptr, "_PonyGObjectTypeString_", "GMenuItem")
    GObjectP.set_data_p[GMenuItem](ptr, "_PonyGObjectPonyObject_", this)

  // GMenuItem
  fun ref getptr(): NullablePointer[GObjectT] => ptr
