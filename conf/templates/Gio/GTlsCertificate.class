
use @g_tls_certificate_get_type[U64]()

use "../GLib"
use "../GObject"
use "lib:gio-2.0"

class GTlsCertificate is GTlsCertificateI
  var ptr: NullablePointer[GObjectT]

  new create_from_ptr(ptr': NullablePointer[GObjectT]) => ptr = ptr'

  new create() =>
    ptr = GObjectG.gnew(@g_tls_certificate_get_type())
    GObjectP.set_data_p[String](ptr, "_PonyGObjectTypeString_", "GTlsCertificate")
    GObjectP.set_data_p[GTlsCertificate](ptr, "_PonyGObjectPonyObject_", this)

  // GTlsCertificate
  fun ref getptr(): NullablePointer[GObjectT] => ptr
