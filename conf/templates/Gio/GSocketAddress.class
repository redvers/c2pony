
use @g_socket_address_get_type[U64]()

use "../GLib"
use "../GObject"
use "lib:gio-2.0"

class GSocketAddress is GSocketAddressI
  var ptr: NullablePointer[GObjectT]

  new create_from_ptr(ptr': NullablePointer[GObjectT]) => ptr = ptr'

  new create() =>
    ptr = GObjectG.gnew(@g_socket_address_get_type())
    GObjectP.set_data_p[String](ptr, "_PonyGObjectTypeString_", "GSocketAddress")
    GObjectP.set_data_p[GSocketAddress](ptr, "_PonyGObjectPonyObject_", this)

  // GSocketAddress
  fun ref getptr(): NullablePointer[GObjectT] => ptr
