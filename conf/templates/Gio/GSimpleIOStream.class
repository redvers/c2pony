
use @g_simple_io_stream_get_type[U64]()

use "../GLib"
use "../GObject"
use "lib:gio-2.0"

class GSimpleIOStream is GSimpleIOStreamI
  var ptr: NullablePointer[GObjectT]

  new create_from_ptr(ptr': NullablePointer[GObjectT]) => ptr = ptr'

  new create() =>
    ptr = GObjectG.gnew(@g_simple_io_stream_get_type())
    GObjectP.set_data_p[String](ptr, "_PonyGObjectTypeString_", "GSimpleIOStream")
    GObjectP.set_data_p[GSimpleIOStream](ptr, "_PonyGObjectPonyObject_", this)

  // GSimpleIOStream
  fun ref getptr(): NullablePointer[GObjectT] => ptr
