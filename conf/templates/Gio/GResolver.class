
use @g_resolver_get_type[U64]()

use "../GLib"
use "../GObject"
use "lib:gio-2.0"

class GResolver is GResolverI
  var ptr: NullablePointer[GObjectT]

  new create_from_ptr(ptr': NullablePointer[GObjectT]) => ptr = ptr'

  new create() =>
    ptr = GObjectG.gnew(@g_resolver_get_type())

  // GResolver
  fun ref getptr(): NullablePointer[GObjectT] => ptr
