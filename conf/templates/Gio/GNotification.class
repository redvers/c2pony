
use @g_notification_get_type[U64]()

use "../GLib"
use "../GObject"
use "lib:gio-2.0"

class GNotification is GNotificationI
  var ptr: NullablePointer[GObjectT]

  new create_from_ptr(ptr': NullablePointer[GObjectT]) => ptr = ptr'

  new create() =>
    ptr = GObjectG.gnew(@g_notification_get_type())
    GObjectP.set_data_p[String](ptr, "_PonyGObjectTypeString_", "GNotification")
    GObjectP.set_data_p[GNotification](ptr, "_PonyGObjectPonyObject_", this)

  // GNotification
  fun ref getptr(): NullablePointer[GObjectT] => ptr
