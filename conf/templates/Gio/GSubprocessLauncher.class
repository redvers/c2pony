
use @g_subprocess_launcher_get_type[U64]()

use "../GLib"
use "../GObject"
use "lib:gio-2.0"

class GSubprocessLauncher is GSubprocessLauncherI
  var ptr: NullablePointer[GObjectT]

  new create_from_ptr(ptr': NullablePointer[GObjectT]) => ptr = ptr'

  new create() =>
    ptr = GObjectG.gnew(@g_subprocess_launcher_get_type())
    GObjectP.set_data_p[String](ptr, "_PonyGObjectTypeString_", "GSubprocessLauncher")
    GObjectP.set_data_p[GSubprocessLauncher](ptr, "_PonyGObjectPonyObject_", this)

  // GSubprocessLauncher
  fun ref getptr(): NullablePointer[GObjectT] => ptr
