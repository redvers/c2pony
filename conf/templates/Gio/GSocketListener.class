
use @g_socket_listener_get_type[U64]()

use "../GLib"
use "../GObject"
use "lib:gio-2.0"

class GSocketListener is GSocketListenerI
  var ptr: NullablePointer[GObjectT]

  new create_from_ptr(ptr': NullablePointer[GObjectT]) => ptr = ptr'

  new create() =>
    ptr = GObjectG.gnew(@g_socket_listener_get_type())
    GObjectP.set_data_p[String](ptr, "_PonyGObjectTypeString_", "GSocketListener")
    GObjectP.set_data_p[GSocketListener](ptr, "_PonyGObjectPonyObject_", this)

  // GSocketListener
  fun ref getptr(): NullablePointer[GObjectT] => ptr
