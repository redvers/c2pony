
use @g_dbus_method_invocation_get_type[U64]()

use "../GLib"
use "../GObject"
use "lib:gio-2.0"

class GDBusMethodInvocation is GDBusMethodInvocationI
  var ptr: NullablePointer[GObjectT]

  new create_from_ptr(ptr': NullablePointer[GObjectT]) => ptr = ptr'

  new create() =>
    ptr = GObjectG.gnew(@g_dbus_method_invocation_get_type())
    GObjectP.set_data_p[String](ptr, "_PonyGObjectTypeString_", "GDBusMethodInvocation")
    GObjectP.set_data_p[GDBusMethodInvocation](ptr, "_PonyGObjectPonyObject_", this)

  // GDBusMethodInvocation
  fun ref getptr(): NullablePointer[GObjectT] => ptr
