
use @g_io_stream_get_type[U64]()

use "../GLib"
use "../GObject"
use "lib:gio-2.0"

class GIOStream is GIOStreamI
  var ptr: NullablePointer[GObjectT]

  new create_from_ptr(ptr': NullablePointer[GObjectT]) => ptr = ptr'

  new create() =>
    ptr = GObjectG.gnew(@g_io_stream_get_type())
    GObjectP.set_data_p[String](ptr, "_PonyGObjectTypeString_", "GIOStream")
    GObjectP.set_data_p[GIOStream](ptr, "_PonyGObjectPonyObject_", this)

  // GIOStream
  fun ref getptr(): NullablePointer[GObjectT] => ptr
