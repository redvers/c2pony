
use @g_dbus_server_get_type[U64]()

use "../GLib"
use "../GObject"
use "lib:gio-2.0"

class GDBusServer is GDBusServerI
  var ptr: NullablePointer[GObjectT]

  new create_from_ptr(ptr': NullablePointer[GObjectT]) => ptr = ptr'

  new create() =>
    ptr = GObjectG.gnew(@g_dbus_server_get_type())
    GObjectP.set_data_p[String](ptr, "_PonyGObjectTypeString_", "GDBusServer")
    GObjectP.set_data_p[GDBusServer](ptr, "_PonyGObjectPonyObject_", this)

  // GDBusServer
  fun ref getptr(): NullablePointer[GObjectT] => ptr
