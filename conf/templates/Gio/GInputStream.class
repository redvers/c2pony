
use @g_input_stream_get_type[U64]()

use "../GLib"
use "../GObject"
use "lib:gio-2.0"

class GInputStream is GInputStreamI
  var ptr: NullablePointer[GObjectT]

  new create_from_ptr(ptr': NullablePointer[GObjectT]) => ptr = ptr'

  new create() =>
    ptr = GObjectG.gnew(@g_input_stream_get_type())
    GObjectP.set_data_p[String](ptr, "_PonyGObjectTypeString_", "GInputStream")
    GObjectP.set_data_p[GInputStream](ptr, "_PonyGObjectPonyObject_", this)

  // GInputStream
  fun ref getptr(): NullablePointer[GObjectT] => ptr
