
use @g_tcp_connection_get_type[U64]()

use "../GLib"
use "../GObject"
use "lib:gio-2.0"

class GTcpConnection is GTcpConnectionI
  var ptr: NullablePointer[GObjectT]

  new create_from_ptr(ptr': NullablePointer[GObjectT]) => ptr = ptr'

  new create() =>
    ptr = GObjectG.gnew(@g_tcp_connection_get_type())
    GObjectP.set_data_p[String](ptr, "_PonyGObjectTypeString_", "GTcpConnection")
    GObjectP.set_data_p[GTcpConnection](ptr, "_PonyGObjectPonyObject_", this)

  // GTcpConnection
  fun ref getptr(): NullablePointer[GObjectT] => ptr
