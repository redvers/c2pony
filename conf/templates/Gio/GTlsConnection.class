
use @g_tls_connection_get_type[U64]()

use "../GLib"
use "../GObject"
use "lib:gio-2.0"

class GTlsConnection is GTlsConnectionI
  var ptr: NullablePointer[GObjectT]

  new create_from_ptr(ptr': NullablePointer[GObjectT]) => ptr = ptr'

  new create() =>
    ptr = GObjectG.gnew(@g_tls_connection_get_type())
    GObjectP.set_data_p[String](ptr, "_PonyGObjectTypeString_", "GTlsConnection")
    GObjectP.set_data_p[GTlsConnection](ptr, "_PonyGObjectPonyObject_", this)

  // GTlsConnection
  fun ref getptr(): NullablePointer[GObjectT] => ptr
