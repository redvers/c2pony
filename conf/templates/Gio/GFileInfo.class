
use @g_file_info_get_type[U64]()

use "../GLib"
use "../GObject"
use "lib:gio-2.0"

class GFileInfo is GFileInfoI
  var ptr: Pointer[GObjectP]

  new create_from_ptr(ptr': Pointer[GObjectP]) => ptr = ptr'
// g_file_info_get_type
  new create() =>
    ptr = GObjectG.gnew(@g_file_info_get_type())

  fun ref getptr(): Pointer[GObjectP] => ptr
