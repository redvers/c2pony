
use @g_settings_get_type[U64]()

use "../GLib"
use "../GObject"
use "lib:gio-2.0"

class GSettings is GSettingsI
  var ptr: NullablePointer[GObjectT]

  new create_from_ptr(ptr': NullablePointer[GObjectT]) => ptr = ptr'

  new create() =>
    ptr = GObjectG.gnew(@g_settings_get_type())
    GObjectP.set_data_p[String](ptr, "_PonyGObjectTypeString_", "GSettings")
    GObjectP.set_data_p[GSettings](ptr, "_PonyGObjectPonyObject_", this)

  // GSettings
  fun ref getptr(): NullablePointer[GObjectT] => ptr
