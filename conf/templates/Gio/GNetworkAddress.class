
use @g_network_address_get_type[U64]()

use "../GLib"
use "../GObject"
use "lib:gio-2.0"

class GNetworkAddress is GNetworkAddressI
  var ptr: NullablePointer[GObjectT]

  new create_from_ptr(ptr': NullablePointer[GObjectT]) => ptr = ptr'

  new create() =>
    ptr = GObjectG.gnew(@g_network_address_get_type())
    GObjectP.set_data_p[String](ptr, "_PonyGObjectTypeString_", "GNetworkAddress")
    GObjectP.set_data_p[GNetworkAddress](ptr, "_PonyGObjectPonyObject_", this)

  // GNetworkAddress
  fun ref getptr(): NullablePointer[GObjectT] => ptr
