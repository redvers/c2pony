
use @g_charset_converter_get_type[U64]()

use "../GLib"
use "../GObject"
use "lib:gio-2.0"

class GCharsetConverter is GCharsetConverterI
  var ptr: NullablePointer[GObjectT]

  new create_from_ptr(ptr': NullablePointer[GObjectT]) => ptr = ptr'

  new create() =>
    ptr = GObjectG.gnew(@g_charset_converter_get_type())
    GObjectP.set_data_p[String](ptr, "_PonyGObjectTypeString_", "GCharsetConverter")
    GObjectP.set_data_p[GCharsetConverter](ptr, "_PonyGObjectPonyObject_", this)

  // GCharsetConverter
  fun ref getptr(): NullablePointer[GObjectT] => ptr
