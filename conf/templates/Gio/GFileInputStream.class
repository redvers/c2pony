
use @g_file_input_stream_get_type[U64]()

use "../GLib"
use "../GObject"
use "lib:gio-2.0"

class GFileInputStream is GFileInputStreamI
  var ptr: NullablePointer[GObjectT]

  new create_from_ptr(ptr': NullablePointer[GObjectT]) => ptr = ptr'

  new create() =>
    ptr = GObjectG.gnew(@g_file_input_stream_get_type())
    GObjectP.set_data_p[String](ptr, "_PonyGObjectTypeString_", "GFileInputStream")
    GObjectP.set_data_p[GFileInputStream](ptr, "_PonyGObjectPonyObject_", this)

  // GFileInputStream
  fun ref getptr(): NullablePointer[GObjectT] => ptr
