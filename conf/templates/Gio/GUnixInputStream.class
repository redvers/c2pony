use "lib:gio-2.0"

class GUnixInputStream is GUnixInputStreamI
  var ptr: NullablePointer[GObjectT]

  new create_from_ptr(ptr': NullablePointer[GObjectT]) => ptr = ptr'

  new create() =>
    ptr = GObjectG.gnew(@g_unix_input_stream_get_type())
    GObjectP.set_data_p[String](ptr, "_PonyGObjectTypeString_", "GUnixInputStream")
    GObjectP.set_data_p[GUnixInputStream](ptr, "_PonyGObjectPonyObject_", this)

  // GUnixInputStream
  fun ref getptr(): NullablePointer[GObjectT] => ptr
