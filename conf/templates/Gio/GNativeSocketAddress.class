
use @g_native_socket_address_get_type[U64]()

use "../GLib"
use "../GObject"
use "lib:gio-2.0"

class GNativeSocketAddress is GNativeSocketAddressI
  var ptr: NullablePointer[GObjectT]

  new create_from_ptr(ptr': NullablePointer[GObjectT]) => ptr = ptr'

  new create() =>
    ptr = GObjectG.gnew(@g_native_socket_address_get_type())
    GObjectP.set_data_p[String](ptr, "_PonyGObjectTypeString_", "GNativeSocketAddress")
    GObjectP.set_data_p[GNativeSocketAddress](ptr, "_PonyGObjectPonyObject_", this)

  // GNativeSocketAddress
  fun ref getptr(): NullablePointer[GObjectT] => ptr
