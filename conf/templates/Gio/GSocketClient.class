
use @g_socket_client_get_type[U64]()

use "../GLib"
use "../GObject"
use "lib:gio-2.0"

class GSocketClient is GSocketClientI
  var ptr: NullablePointer[GObjectT]

  new create_from_ptr(ptr': NullablePointer[GObjectT]) => ptr = ptr'

  new create() =>
    ptr = GObjectG.gnew(@g_socket_client_get_type())

  // GSocketClient
  fun ref getptr(): NullablePointer[GObjectT] => ptr
