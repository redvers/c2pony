
use @g_cancellable_get_type[U64]()

use "../GLib"
use "../GObject"
use "lib:gio-2.0"

class GCancellable is GCancellableI
  var ptr: NullablePointer[GObjectT]

  new create_from_ptr(ptr': NullablePointer[GObjectT]) => ptr = ptr'

  new create() =>
    ptr = GObjectG.gnew(@g_cancellable_get_type())
    GObjectP.set_data_p[String](ptr, "_PonyGObjectTypeString_", "GCancellable")
    GObjectP.set_data_p[GCancellable](ptr, "_PonyGObjectPonyObject_", this)

  // GCancellable
  fun ref getptr(): NullablePointer[GObjectT] => ptr
