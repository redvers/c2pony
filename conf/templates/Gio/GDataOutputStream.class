
use @g_data_output_stream_get_type[U64]()

use "../GLib"
use "../GObject"
use "lib:gio-2.0"

class GDataOutputStream is GDataOutputStreamI
  var ptr: NullablePointer[GObjectT]

  new create_from_ptr(ptr': NullablePointer[GObjectT]) => ptr = ptr'

  new create() =>
    ptr = GObjectG.gnew(@g_data_output_stream_get_type())
    GObjectP.set_data_p[String](ptr, "_PonyGObjectTypeString_", "GDataOutputStream")
    GObjectP.set_data_p[GDataOutputStream](ptr, "_PonyGObjectPonyObject_", this)

  // GDataOutputStream
  fun ref getptr(): NullablePointer[GObjectT] => ptr
