
use @g_zlib_compressor_get_type[U64]()

use "../GLib"
use "../GObject"
use "lib:gio-2.0"

class GZlibCompressor is GZlibCompressorI
  var ptr: NullablePointer[GObjectT]

  new create_from_ptr(ptr': NullablePointer[GObjectT]) => ptr = ptr'

  new create() =>
    ptr = GObjectG.gnew(@g_zlib_compressor_get_type())
    GObjectP.set_data_p[String](ptr, "_PonyGObjectTypeString_", "GZlibCompressor")
    GObjectP.set_data_p[GZlibCompressor](ptr, "_PonyGObjectPonyObject_", this)

  // GZlibCompressor
  fun ref getptr(): NullablePointer[GObjectT] => ptr
