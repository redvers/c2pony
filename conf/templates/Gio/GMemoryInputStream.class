
use @g_memory_input_stream_get_type[U64]()

use "../GLib"
use "../GObject"
use "lib:gio-2.0"

class GMemoryInputStream is GMemoryInputStreamI
  var ptr: NullablePointer[GObjectT]

  new create_from_ptr(ptr': NullablePointer[GObjectT]) => ptr = ptr'

  new create() =>
    ptr = GObjectG.gnew(@g_memory_input_stream_get_type())
    GObjectP.set_data_p[String](ptr, "_PonyGObjectTypeString_", "GMemoryInputStream")
    GObjectP.set_data_p[GMemoryInputStream](ptr, "_PonyGObjectPonyObject_", this)

  // GMemoryInputStream
  fun ref getptr(): NullablePointer[GObjectT] => ptr
