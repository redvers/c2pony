
use @g_initially_unowned_get_type[U64]()

use "../GLib"
use "lib:gobject-2.0"

class GInitiallyUnowned is GInitiallyUnownedI
  var ptr: NullablePointer[GObjectT]

  new create_from_ptr(ptr': NullablePointer[GObjectT]) => ptr = ptr'

  new create() =>
    ptr = GObjectG.gnew(@g_initially_unowned_get_type())
    GObjectP.set_data_p[String](ptr, "_PonyGObjectTypeString_", "GInitiallyUnowned")
    GObjectP.set_data_p[GInitiallyUnowned](ptr, "_PonyGObjectPonyObject_", this)

  // GInitiallyUnowned
  fun ref getptr(): NullablePointer[GObjectT] => ptr
